{
	"name": "Munna's Darkmeda",
	"colors": {
		"editor.background": "#1e2028",
		"editor.lineHighlightBackground": "#44d3c506",
		"editor.wordHighlightBackground": "#848aa860",
		"editor.wordHighlightStrongBackground": "#5b5d6880",
		"sideBar.background": "#1b1e2b",
		"activityBar.background": "#1a1c26",
		"activityBar.activeBackground": "#232635",
		"activityBarBadge.background": "#4bc3a5",
		"debugConsole.errorForeground": "#4bc3a5",
		// "sideBar.border": "#27af8d",


		// sidebar file names and texts
		"sideBarTitle.foreground": "#4bc3a5",
		"sideBar.foreground": "#b9bcbc",
		// "sideBarSectionHeader.foreground": "#4bc3a5",
		"sideBarSectionHeader.background": "#4bc3a50c",
		"sideBarSectionHeader.border": "#2d7563",
		"sideBarTitle.border": "#2d7563",

		// suggestion
		"editorSuggestWidget.highlightForeground": "#4bc3a5",
		"editorSuggestWidget.selectedBackground": "#4cb498",
		"editorSuggestWidget.selectedForeground": "#000000",
		"editorSuggestWidget.focusHighlightForeground": "#000000",
		"editor.compositionBorder": "#54f4cc",
		"badge.background": "#4bc3a5",
		"badge.foreground": "#000000",
		"tab.activeBackground": "#2a2d3a",
		"tab.inactiveBackground": "#181a25",
		"titleBar.activeBackground": "#181a25",
		"panel.background": "#1b1e2b",
		"scrollbarSlider.background": "#5c5c5c30",
		"scrollbarSlider.hoverBackground": "#7f7f7f40",
		"scrollbarSlider.activeBackground": "#a0a0a050",
		"titleBar.activeForeground": "#dcede9",


		// tab
		"tab.activeForeground": "#4bc3a5",
		// "tab.activeBorderTop": "#4bc3a5",
		"tab.activeBorder": "#4bc3a5",
		"tab.hoverForeground": "#4bc3a5",
		"tab.inactiveForeground": "#a7aeac",
		"activityBar.foreground": "#5af5ce",


		// terminal
		// "terminal.foreground": "#4bc3a5",
		"terminalCursor.foreground": "#4bc3a5",
		
		"list.activeSelectionBackground": "#60746f81",
		"list.inactiveSelectionForeground": "#4bc3a5",
		

		"activityBar.inactiveForeground": "#777777", // ইনঅ্যাকটিভ লেখা রঙ-এর ব্যাকগ্রাউন্ড (এইটা কিছু থিমে দেখা যায় না)
		"activityBar.activeBorder": "#5af5ce", //  // badge (সংখ্যা) ব্যাকগ্রাউন্ড (যেমন 1 update pending)
		"activityBarBadge.foreground": "#000000",
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable": "#4bc3a5",
		"property": "#c7975e",
		"parameter": "#4bc3a5",
		"class": "#d8893a",
		"enum": "#C792EA",
		"*.declaration": "#4bc3a5"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#8c8d8b"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ffb97f"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#c490e2"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#d4a73d"
			}
		},
		{
			"scope": [
				"variable.other",
				"variable",
				"variable.parameter",
				"support.variable.property",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#4bc3a5"
			}
		},
		{
			"scope": [
				"string.quoted",
				"string.template"
			],
			"settings": {
				"foreground": "#5ab252"
			}
		},
		{
			"scope": "string.quoted.html",
			"settings": {
				"foreground": "#5cae55"
			}
		},
		{
			"scope": [
				"storage.type",
				"keyword.control.import",
				"storage.modifier.async"
			],
			"settings": {
				"foreground": "#c490e2"
			}
		},
		{
			"scope": [
				"punctuation.accessor",
				"entity.name.tag.html"
			],
			"settings": {
				"foreground": "#d8893a"
			}
		},
		{
			"scope": "text.html.derivative",
			"settings": {
				"foreground": "#b7bcb0ee"
			}
		},
		{
			"scope": "comment.block.html",
			"settings": {
				"foreground": "#8c7a93ee"
			}
		},
		{
			"scope": "meta.property-value.css",
			"settings": {
				"foreground": "#dfb26bee"
			}
		},
		{
			"scope": "entity.other.attribute-name.id.css",
			"settings": {
				"foreground": "#dfb26bee"
			}
		},
		{
			"scope": "entity.other.attribute-name.class.css",
			"settings": {
				"foreground": "#c4c23dee"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#e9d414"
			}
		},
		{
			"scope": "storage.type.function.js",
			"settings": {
				"foreground": "#ff6f6f"
			}
		},
		{
			"scope": [
				"keyword.control.conditional",
				"constant.language.boolean.true",
				"constant.language.boolean.false"
			],
			"settings": {
				"foreground": "#e05353"
			}
		},
		{
			"scope": "keyword.control.loop.js",
			"settings": {
				"foreground": "#e05353"
			}
		},
		{
			"scope": "support.constant.property-value.css",
			"settings": {
				"foreground": "#d4c818"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#c490e2"
			}
		},
		{
			"scope": [
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#b4b4b4ee"
			}
		},
		{
			"scope": "punctuation.definition.entity.html",
			"settings": {
				"foreground": "#f54717ee"
			}
		},
		{
			"scope": "entity.name.tag.jsx",
			"settings": {
				"foreground": "#917e88",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"entity.name.tag.js",
				"constant.language.null.js.jsx"
			],
			"settings": {
				"foreground": "#c34416",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "entity.name.tag.tsx",
			"settings": {
				"foreground": "#FF79C6"
			}
		},
		{
			"scope": "entity.name.tag.end.jsx",
			"settings": {
				"foreground": "#FF79C6",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "entity.other.attribute-name.jsx",
			"settings": {
				"foreground": "#F1FA8C"
			}
		},
		{
			"scope": "meta.jsx.children.js.jsx",
			"settings": {
				"foreground": "#c1c1c1"
			}
		},
		{
			"scope": "support.class.component.js",
			"settings": {
				"foreground": "#c123a9",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "support.type.property-name.json.comments",
			"settings": {
				"foreground": "#4bc3a5",
				"fontStyle": ""
			}
		},
		{
			"scope": "punctuation.definition.tag.end.js.jsx",
			"settings": {
				"foreground": "#b6a175",
			}
		},
		{
			"scope": "punctuation.definition.tag.begin.js.jsx",
			"settings": {
				"foreground": "#b49f74",
			}
		},
	]
}